                                                                  owl:complementOf :Proposal
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ]
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( [ owl:intersectionOf ( :Proposal
                                                               [ rdf:type owl:Class ;
                                                                 owl:complementOf :Variant
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ]
                                        [ owl:intersectionOf ( :Variant
                                                               [ rdf:type owl:Class ;
                                                                 owl:complementOf :Proposal
                                                               ]
                                                             ) ;
