:ApplicationExample rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain :Proposal ;
                    rdfs:range rdfs:Literal ;
                    rdfs:label "Application example" .


